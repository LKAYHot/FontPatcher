  <Style Selector="ComboBox.field:focus">
    <Setter Property="BorderBrush" Value="#3B82F6" />
  </Style>

  <Style Selector="CheckBox.switch">
    <Setter Property="Width" Value="40" />
    <Setter Property="Height" Value="22" />
    <Setter Property="Template">
      <ControlTemplate>
        <Grid Width="40" Height="22">
          <Border x:Name="Track"
                  CornerRadius="11"
                  Background="#475569" />
          <Border x:Name="Thumb"
                  Width="16"
                  Height="16"
                  CornerRadius="8"
                  Background="#E2E8F0"
                  HorizontalAlignment="Left"
                  Margin="3" />
        </Grid>
      </ControlTemplate>
    </Setter>
  </Style>

  <Style Selector="CheckBox.switch:checked /template/ Border#Track">
    <Setter Property="Background" Value="#3B82F6" />
  </Style>

  <Style Selector="CheckBox.switch:checked /template/ Border#Thumb">
    <Setter Property="HorizontalAlignment" Value="Right" />
  </Style>

  <Style Selector="ProgressBar.console">
    <Setter Property="Height" Value="4" />
    <Setter Property="Width" Value="168" />
    <Setter Property="Foreground" Value="#3B82F6" />
    <Setter Property="Background" Value="#1F2937" />
  </Style>

  <Style Selector="ListBox.console-logs">
    <Setter Property="Background" Value="Transparent" />
    <Setter Property="BorderThickness" Value="0" />
    <Setter Property="Padding" Value="0" />
  </Style>

  <Style Selector="controls|FileDropZone">
    <Setter Property="Cursor" Value="Hand" />
  </Style>

  <Style Selector="controls|FileDropZone Border#Surface">
    <Setter Property="Background" Value="#0C162D" />
    <Setter Property="Transitions">
      <Transitions>
        <BrushTransition Property="Background" Duration="0:0:0.18" />
      </Transitions>
    </Setter>
  </Style>

  <Style Selector="controls|FileDropZone Rectangle#DashRect">
    <Setter Property="Stroke" Value="#23324B" />
    <Setter Property="Transitions">
      <Transitions>
        <BrushTransition Property="Stroke" Duration="0:0:0.18" />
      </Transitions>
    </Setter>
  </Style>

  <Style Selector="controls|FileDropZone Border#IconCircle">
    <Setter Property="Background" Value="#1C2A44" />
    <Setter Property="RenderTransformOrigin" Value="50%,50%" />
    <Setter Property="RenderTransform" Value="scale(1)" />
    <Setter Property="Transitions">
      <Transitions>
        <BrushTransition Property="Background" Duration="0:0:0.18" />
        <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.18" />
      </Transitions>
    </Setter>
  </Style>

  <Style Selector="controls|FileDropZone Path#IconPath">
    <Setter Property="Stroke" Value="#8AA0BE" />
    <Setter Property="RenderTransformOrigin" Value="50%,50%" />
    <Setter Property="RenderTransform" Value="scale(1)" />
    <Setter Property="Transitions">
      <Transitions>
        <BrushTransition Property="Stroke" Duration="0:0:0.18" />
        <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.18" />
      </Transitions>
    </Setter>
  </Style>

  <Style Selector="controls|FileDropZone TextBlock#PrimaryText">
    <Setter Property="Foreground" Value="#CFD9E8" />
  </Style>

  <Style Selector="controls|FileDropZone TextBlock#SecondaryText">
    <Setter Property="Foreground" Value="#5F738F" />
  </Style>

  <Style Selector="controls|FileDropZone Border#CheckIcon">
    <Setter Property="Background" Value="#3B82F6" />
  </Style>

  <Style Selector="controls|FileDropZone.empty:pointerover Border#Surface">
    <Setter Property="Background" Value="#11223E" />
  </Style>

  <Style Selector="controls|FileDropZone.empty:pointerover Rectangle#DashRect">
    <Setter Property="Stroke" Value="#365A84" />
  </Style>

  <Style Selector="controls|FileDropZone:pointerover Border#IconCircle">
    <Setter Property="RenderTransform" Value="scale(1.06)" />
  </Style>

  <Style Selector="controls|FileDropZone:pointerover Path#IconPath">
    <Setter Property="RenderTransform" Value="scale(1.08)" />
  </Style>

  <Style Selector="controls|FileDropZone.filled Rectangle#DashRect">
    <Setter Property="Stroke" Value="#2F78FF" />
  </Style>

  <Style Selector="controls|FileDropZone.filled Border#IconCircle">
    <Setter Property="Background" Value="#3B82F6" />
  </Style>

  <Style Selector="controls|FileDropZone.filled Path#IconPath">
    <Setter Property="Stroke" Value="White" />
  </Style>

  <Style Selector="controls|FileDropZone.filled TextBlock#PrimaryText">
    <Setter Property="Foreground" Value="#3B82F6" />
  </Style>

  <Style Selector="controls|FileDropZone.dragging Border#Surface">
    <Setter Property="Background" Value="#1E3A8A2C" />
  </Style>

  <Style Selector="controls|FileDropZone.dragging Rectangle#DashRect">
    <Setter Property="Stroke" Value="#60A5FA" />
  </Style>
</Styles>
